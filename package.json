{
    "name": "vscode-pos-liquid",
    "displayName": "PlatformOS liquid Language Support",
    "description": "PlatformOS Liquid language Syntax Highlighter for Visual Studio Code.",
    "version": "0.0.1",
    "publisher": "digitalfuel",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "pos-liquid",
            "aliases": ["HTML pos-liquid", "pos-liquid"],
            "extensions": [".liquid"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "pos-liquid",
            "scopeName": "text.html.liquid",
            "path": "./syntaxes/liquid.tmLanguage.json"
        }]
    }
}